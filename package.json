{
  "name": "chat-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "tsx db/postgres/seed/execute.seed.ts",
    "test": "npm run test:unit && npm run test:integration",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "PW_TEST_CONNECT_WS_ENDPOINT=ws://localhost:5000/ npx playwright test",
    "playwright": "npm run test:integration",
    "playwright:ui": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:5000/ npx playwright test --ui-port=6007",
    "test:unit": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "migrator": "tsx db/postgres/tools/migrator-cli.ts",
    "node-server": "node --env-file=.env --import=tsx prodServer.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.44.1",
    "@storybook/addon-a11y": "^8.1.3",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-svelte-csf": "^4.1.2",
    "@storybook/blocks": "^8.0.9",
    "@storybook/svelte": "^8.0.9",
    "@storybook/sveltekit": "^8.0.9",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^5.1.0",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.38.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^14.7.1",
    "kysely-codegen": "^0.15.0",
    "msw": "^2.2.14",
    "msw-storybook-addon": "^2.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.75.0",
    "storybook": "^8.0.9",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.0",
    "sveltekit-superforms": "^2.12.6",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "type": "module",
  "dependencies": {
    "@iconify/icons-eos-icons": "^1.2.6",
    "@iconify/icons-humbleicons": "^1.2.4",
    "@iconify/icons-iconoir": "^1.2.30",
    "@iconify/svelte": "^3.1.4",
    "@storybook/test": "^8.0.9",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "kysely": "^0.27.3",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "winston": "^3.13.0",
    "zod": "^3.23.4"
  }
}

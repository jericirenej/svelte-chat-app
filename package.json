{
  "name": "chat-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "tsx db/postgres/seed/execute.seed.ts",
    "test": "npm run test:unit && npm run test:integration",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:5000/ npx playwright test",
    "playwright:ui": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:5000/ npx playwright test --ui-port=6007",
    "test:unit": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "migrator": "tsx db/postgres/tools/migrator-cli.ts",
    "node-server": "node --env-file=.env --import=tsx prodServer.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-actions": "^8.0.6",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-svelte-csf": "^4.0.9",
    "@storybook/blocks": "^7.4.0",
    "@storybook/svelte": "^7.4.0",
    "@storybook/sveltekit": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.30.3",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^10.11.2",
    "kysely-codegen": "^0.14.0",
    "msw": "^1.2.5",
    "msw-storybook-addon": "^1.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.0",
    "storybook": "^7.4.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "^1.13.4",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "tsx": "^4.3.0",
    "typescript": "^5.2.0",
    "uuid": "^9.0.1",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@iconify/icons-eos-icons": "^1.2.6",
    "@iconify/icons-humbleicons": "^1.2.4",
    "@iconify/icons-iconoir": "^1.2.30",
    "@iconify/svelte": "^3.1.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "kysely": "^0.27.2",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
